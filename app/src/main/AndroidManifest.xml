<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.licheedev.blurviewproject" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StaticBlurActivity"
            android:label="@string/title_activity_static_blur" >
        </activity>
        <activity
            android:name=".LiveBlurActivity"
            android:label="@string/title_activity_live_blur" >
        </activity>
        <activity
            android:name=".BaseTestActivity"
            android:label="@string/title_activity_base_test" >
        </activity>
    </application>

</manifest>
